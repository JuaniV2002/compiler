program {
    integer inc(integer x) {
        if (x < 2) then {
            return x + 1;
        } else {
            x = x + 10;
        }

        return x;
    }

    integer get_int() extern;

    void print_int(integer i) extern;

    void main() {
        integer y = 0;
        y = get_int();
        y = get_int();

        if (y == 1) then {
            print_int(y);
        } else {
            return print_int(inc(y));
        }
    }
}